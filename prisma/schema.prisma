generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  cpf       String?  @unique
  cnpj      String?  @unique
  password  String
  type      Perfil
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


enum Perfil {
  ADMIN
  ONG
  CLIENT
}
